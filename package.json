{
  "name": "mentees_server",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "server": "nodemon --watch server/ --exec \"npx\" \"ts-node\" server/index.ts",
    "client": "cd client/ && yarn start",
    "start": "concurrently --kill-others \"yarn server\" \" yarn client\""
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.14",
    "express": "^4.18.1",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "concurrently": "^7.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "mysql": "^2.18.1"
  }
}
